<?xml version='1.0' encoding='utf-8'?>
<widget id="com.emlautarom1.flappy.bird.js" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Flappy Bird JS</name>
    <description>
        Simple Flappy Birds clone built in plain JavaScript/HTML/CSS.
    </description>
    <author email="emlautarom1@gmail.com" href="https://github.com/emlautarom1">
        Lautaro Martin Emanuel
    </author>
    <content src="index.html" />
    <plugin name="cordova-plugin-whitelist" spec="1" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
</widget>
